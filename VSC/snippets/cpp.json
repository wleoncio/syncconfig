	"Add temporary R cout": {
		"prefix": "rcout",
		"body": "Rcpp::Rcout << \"$1\\n\" << $1 << std::endl; //TEMP"
	},
	"Add temporary cout": {
		"prefix": "cout",
		"body": "std::cout << \"$1\\n\" << $1 << std::endl; //TEMP"
	},
	"Add temporary message": {
		"prefix": "tmp",
		"body": "Rcpp::Rcout << \"$1\" << std::endl; //TEMP"
	}
}
