{
    // Language-specific
    "[git-commit]": {
        "editor.rulers": [
            {
                "color": "#ff0000",
                "column": 50
            }
        ]
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.rulers": []
    },
    "alignment.chars": {
        "<-": {
            "spaceAfter": 1,
            "spaceBefore": 1
        }
    },
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "off",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "blink",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.cursorWidth": 3,
    "editor.emptySelectionClipboard": false,
    "editor.find.loop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "editor.fontWeight": "normal",
    "editor.guides.bracketPairs": "active",
    "editor.inlayHints.enabled": "off",
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": false,
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.minimap.side": "left",
    "editor.minimap.size": "fit",
    "editor.mouseWheelZoom": false,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.renderLineHighlight": "all",
    "editor.rulers": [
        {
            "color": "#ff0000",
            "column": 80
        }
    ],
    "editor.scrollBeyondLastColumn": 0,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "textMateRules": [
                // Find scope values by running the following on the code file:
                // Developer: Inspect Editor Tokens and Scopes
                {
                    "scope": [
                        "meta.function-call.arguments.r",
                        "keyword.operator.assignment.r",
                        "punctuation.separator.parameters.r"
                    ],
                    "settings": {
                        "foreground": "#f29668"
                    }
                },
                {
                    "scope": [
                        "constant.language.r"
                    ],
                    "settings": {
                        "foreground": "#7e7e7e"
                    }
                },
                {
                    "scope": [
                        "constant.numeric.float.decimal.r",
                        "constant.numeric.integer.decimal.r"
                    ],
                    "settings": {
                        "foreground": "#5153b3"
                    }
                },
                {
                    "scope": [
                        "support.function.r"
                    ],
                    "settings": {
                        "foreground": "#4381c9"
                    }
                },
                {
                    "scope": [
                        "variable.parameter.r",
                        "variable.other.r"
                    ],
                    "settings": {
                        "foreground": "#dfeeff"
                    }
                },
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#6b6de4"
                    }
                }
            ]
        }
    },
    "editor.wordWrap": "off",
    "excalidraw.theme": "light",
    "explorer.compactFolders": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.docker": "Dockerfile"
    },
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git-graph.commitDetailsView.location": "Docked to Bottom",
    "git-graph.dialog.createBranch.checkOut": true,
    "git-graph.repository.commits.fetchAvatars": true,
    "git-graph.repository.fetchAndPrune": true,
    "git-graph.showStatusBarItem": false,
    "git.autofetch": true,
    "git.checkoutType": [
        "local"
    ],
    "git.openRepositoryInParentFolders": "never",
    "git.suggestSmartCommit": false,
    "github-actions.remote-name": "ocbe-uio",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false
    },
    "gitweblinks.defaultBranch": "develop",
    "gitweblinks.preferredRemoteName": "ocbe-uio",
    "html.preview.doubleClickToSwitchToEditor": false,
    "julia.additionalArgs": [
        "--threads=4"
    ],
    "julia.enableTelemetry": false,
    "julia.numTestProcesses": 1,
    "julia.NumThreads": 6,
    "julia.symbolCacheDownload": true,
    "kotlin.debugAdapter.enabled": false,
    "kotlin.languageServer.enabled": false,
    "markdown.extension.print.theme": "dark",
    "markdown.extension.tableFormatter.enabled": true,
    "markdown.extension.toc.levels": "1..1",
    "markdown.extension.toc.orderedList": true,
    "markdown.marp.enableHtml": true,
    "markdown.marp.exportType": "html",
    "markdown.marp.themes": [
        "./themes/uio-light.css",
        "./themes/uio-dark.css"
    ],
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "matlab.lintOnSave": false,
    "notebook.lineNumbers": "on",
    "outline.icons": false,
    "python.defaultInterpreterPath": "/home/waldir/.local/bin/bpython",
    "r.bracketedPaste": true,
    "r.lsp.diagnostics": false,
    "r.lsp.enabled": false,
    "r.rpath.linux": "/usr/bin/R",
    "r.rterm.linux": "/usr/bin/radian",
    "r.source.focus": "terminal",
    "remote.SSH.configFile": "/home/waldir/.ssh/config",
    "scm.defaultViewMode": "tree",
    "security.workspace.trust.untrustedFiles": "open",
    "task.saveBeforeRun": "prompt",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "terminal.integrated.confirmOnExit": "always",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.defaultProfile.linux": "fish",
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.fontFamily": "FiraCode Nerd Font",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeight": "500",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "icon": "terminal-bash",
            "path": "bash"
        },
        "fish": {
            "color": "terminal.ansiWhite",
            "icon": "terminal-linux",
            "path": "fish"
        },
        "R": {
            "color": "terminal.ansiBlue",
            "icon": "package",
            "overrideName": true,
            "path": "/usr/bin/R",
        },
        "radian": {
            "color": "terminal.ansiBlue",
            "icon": "package",
            "overrideName": true,
            "path": "/home/waldir/.local/bin/radian"
        }
    },
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.tabs.focusMode": "singleClick",
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "terminal.integrated.tabs.title": "${process}",
    "todo-tree.general.revealBehaviour": "start of todo",
    "todo-tree.general.statusBar": "tags",
    "todo-tree.general.statusBarClickBehaviour": "reveal",
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "TEMP",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.customHighlight": {
        "FIXME": {
            "background": "#F00",
            "foreground": "#FFF",
            "gutterIcon": true,
            "icon": "alert",
            "iconColour": "#F00",
            "type": "text"
        },
        "TEMP": {
            "background": "#707070",
            "foreground": "#000",
            "gutterIcon": true,
            "icon": "clock",
            "iconColour": "#707070",
            "type": "line"
        },
        "TODO": {
            "background": "#0F0",
            "foreground": "#000",
            "gutterIcon": true,
            "icon": "check",
            "type": "text"
        }
    },
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "typescript.suggest.paths": false,
    "window.restoreWindows": "none",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f",
        "editorIndentGuide.activeBackground1": "#000",
        "editorIndentGuide.background1": "#000",
        "terminal.ansiBlack": "#000",
        "terminal.ansiBrightBlack": "#fff",
        "terminal.ansiBrightRed": "#fd6464",
        "terminal.ansiGreen": "#00ff00",
        "terminal.ansiRed": "#ff0000",
        "terminal.ansiWhite": "#fff",
        "terminal.ansiYellow": "#ffaa00",
        "terminal.border": "#888"
    },
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.mouseBackForwardToNavigate": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "editor.stickyScroll.enabled": true,
    "notebook.stickyScroll.enabled": true,
    "editor.stickyTabStops": true,
    "window.menuBarVisibility": "toggle",
    "workbench.sideBar.location": "right",
    "markdown-pdf.displayHeaderFooter": false,
    "todo-tree.general.debug": true
}
