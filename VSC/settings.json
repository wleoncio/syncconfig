{
    // Language-specific
    "[git-commit]": {
        "editor.rulers": [
            {
                "color": "#dd0000",
                "column": 50
            }
        ]
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
        "editor.rulers": []
    },
    "background-terminal-notifier.notificationTimeout": 2000,
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "off",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "blink",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.cursorWidth": 3,
    "editor.emptySelectionClipboard": false,
    "editor.find.loop": false,
    "editor.foldingStrategy": "indentation",
    "editor.fontFamily": "'FiraCode Nerd Font'",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.fontWeight": "700",
    "editor.guides.bracketPairs": "active",
    "editor.inlayHints.enabled": "off",
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": false,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 80,
    "editor.minimap.showSlider": "always",
    "editor.minimap.side": "left",
    "editor.minimap.size": "fit",
    "editor.mouseWheelZoom": false,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestionsDelay": 0,
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.rulers": [
        {
            "color": "#e6ecff",
            "column": 80
        }
    ],
    "editor.scrollBeyondLastColumn": 0,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.stickyScroll.enabled": true,
    "editor.stickyTabStops": true,
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "textMateRules": [
                // Find scope values by running the following on the code file:
                // Developer: Inspect Editor Tokens and Scopes
                {
                    "scope": [
                        "meta.function-call.arguments.r",
                        "keyword.operator.assignment.r",
                        "punctuation.separator.parameters.r"
                    ],
                    "settings": {
                        "foreground": "#f29668"
                    }
                },
                {
                    "scope": [
                        "constant.language.r"
                    ],
                    "settings": {
                        "foreground": "#7e7e7e"
                    }
                },
                {
                    "scope": [
                        "constant.numeric.float.decimal.r",
                        "constant.numeric.integer.decimal.r"
                    ],
                    "settings": {
                        "foreground": "#5153b3"
                    }
                },
                {
                    "scope": [
                        "support.function.r"
                    ],
                    "settings": {
                        "foreground": "#4381c9"
                    }
                },
                {
                    "scope": [
                        "variable.parameter.r",
                        "variable.other.r"
                    ],
                    "settings": {
                        "foreground": "#dfeeff"
                    }
                },
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#6b6de4"
                    }
                }
            ]
        },
        "comments": "#b2b3b7",
        "functions": "#86a4f7",
        "keywords": "#86a4f7",
        "numbers": "#2ec483",
        "strings": "#fdcb87",
        "textMateRules": [
            {
                "scope": [
                    "keyword.control",
                    "source.css.scss"
                ],
                "settings": {
                    "foreground": "#86a4f7"
                }
            },
            {
                "scope": [
                    "entity.name.function"
                ],
                "settings": {
                    "foreground": "#e6ecff"
                }
            },
            {
                "scope": [
                    "keyword.operator"
                ],
                "settings": {
                    "foreground": "#86a4f7"
                }
            },
            {
                "scope": [
                    "storage.type"
                ],
                "settings": {
                    "foreground": "#86a4f7"
                }
            },
            {
                "scope": [
                    "support.type.property-name",
                    "source.css variable"
                ],
                "settings": {
                    "foreground": "#e6ecff"
                }
            }
        ],
        "types": "#dd0000",
        "variables": "#e6ecff"
    },
    "editor.wordWrap": "off",
    "explorer.compactFolders": false,
    "extendedTerminalIntegration.statusBar.showTerminalName": true,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.docker": "Dockerfile",
        "*.Rnw": "latex",
        "renv.lock": "json"
    },
    "files.autoGuessEncoding": true,
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git-graph.date.format": "Relative",
    "git-graph.repository.commits.fetchAvatars": true,
    "git.autofetch": true,
    "git.checkoutType": [
        "local"
    ],
    "git.openRepositoryInParentFolders": "never",
    "git.suggestSmartCommit": false,
    "github-actions.remote-name": "ocbe-uio",
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "quarto": true,
        "scminput": false
    },
    "markdown.extension.print.theme": "dark",
    "markdown.extension.tableFormatter.enabled": true,
    "markdown.extension.toc.levels": "1..1",
    "markdown.extension.toc.orderedList": true,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "MATLAB.installPath": "/usr/local/MATLAB/R2023b",
    "notebook.lineNumbers": "on",
    "notebook.stickyScroll.enabled": true,
    "outline.icons": false,
    "python.defaultInterpreterPath": "/home/waldir/venv/bin/python3",
    "python.REPL.sendToNativeREPL": true,
    "python.terminal.activateEnvironment": false,
    "quarto.render.previewType": "external",
    "quarto.zotero.library": "none",
    "r.alwaysUseActiveTerminal": true,
    "r.bracketedPaste": true,
    "r.lsp.diagnostics": false,
    "r.lsp.enabled": false,
    "r.rpath.linux": "/usr/bin/R",
    "r.rterm.linux": "${userHome}/.local/bin/radian",
    "remote.SSH.configFile": "/home/waldir/.ssh/config",
    "remote.SSH.connectTimeout": 30,
    "remote.SSH.showLoginTerminal": true,
    "scm.defaultViewMode": "tree",
    "security.workspace.trust.untrustedFiles": "open",
    "task.allowAutomaticTasks": "on",
    "task.saveBeforeRun": "prompt",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.autoReplies": {
        "Save workspace image? [y/n/c]:": "n\r",
        "use a personal library instead? (yes/No/cancel)": "yes\r"
    },
    "terminal.integrated.confirmOnExit": "always",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "terminal.integrated.fontFamily": "'FiraCode Nerd Font'",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.ignoreProcessNames": [
        "starship",
        "oh-my-posh",
        "bash",
        "zsh",
        "fish"
    ],
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.suggest.enabled": true,
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.tabs.focusMode": "singleClick",
    "terminal.integrated.tabs.hideCondition": "never",
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "todo-tree.general.debug": true,
    "todo-tree.general.revealBehaviour": "start of todo",
    "todo-tree.general.statusBar": "tags",
    "todo-tree.general.statusBarClickBehaviour": "reveal",
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "TEMP",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.customHighlight": {
        "FIXME": {
            "background": "#F00",
            "foreground": "#FFF",
            "gutterIcon": true,
            "icon": "alert",
            "iconColour": "#F00",
            "opacity": 40,
            "type": "text"
        },
        "TEMP": {
            "background": "#707070",
            "foreground": "#777",
            "gutterIcon": true,
            "icon": "clock",
            "iconColour": "#707070",
            "opacity": 40,
            "type": "line"
        },
        "TODO": {
            "background": "#0F0",
            "foreground": "#0F3",
            "gutterIcon": true,
            "icon": "check",
            "opacity": 40,
            "type": "text"
        }
    },
    "todo-tree.regex.regex": "(//|#|%|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "typescript.suggest.paths": false,
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.gdefault": true,
    "vim.highlightedyank.enable": true,
    "vim.history": 10,
    "vim.inccommand": "append",
    "vim.matchpairs": "(:),{:},[:]",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "after": [
                ""
            ],
            "before": [
                "="
            ],
            "commands": [
                {
                    "args": [],
                    "command": "editor.action.reindentlines"
                }
            ]
        }
    ],
    "vim.report": 1,
    "vim.smartRelativeLine": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.insert": [
        "#2ec483"
    ],
    "vim.statusBarColors.normal": [
        "#dd0000"
    ],
    "vim.statusBarColors.visual": [
        "#3e31d6"
    ],
    "vim.textwidth": 79,
    "vim.useSystemClipboard": true,
    "window.customTitleBarVisibility": "never",
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "native",
    "workbench.activityBar.location": "hidden",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#1073cf2d",
        "editor.lineHighlightBorder": "#9fced11f",
        "editorBracketHighlight.foreground1": "#2ec483",
        "editorBracketHighlight.foreground2": "#6ce1ab",
        "editorBracketHighlight.foreground3": "#ceffdf",
        "editorBracketHighlight.foreground4": "#fffea7",
        "editorBracketHighlight.foreground5": "#ffe0e0",
        "editorBracketHighlight.foreground6": "#fb6666",
        "editorIndentGuide.activeBackground1": "#000",
        "editorIndentGuide.background1": "#000",
        "extendedTerminalIntegration.statusBar.activeForeground": "#000",
        "extendedTerminalIntegration.statusBar.foreground": "#fff",
        "statusBar.background": "#2ec483",
        "statusBar.debuggingBackground": "#2ec483",
        "statusBar.debuggingForeground": "#ffffff",
        "statusBar.foreground": "#ffffff",
        "statusBar.noFolderBackground": "#2ec483",
        "terminal.ansiBlack": "#000",
        "terminal.ansiBrightBlack": "#fff",
        "terminal.ansiBrightRed": "#fd6464",
        "terminal.ansiGreen": "#00ff00",
        "terminal.ansiRed": "#ff0000",
        "terminal.ansiWhite": "#fff",
        "terminal.ansiYellow": "#ffaa00",
        "terminal.border": "#888"
    },
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.editor.centeredLayoutAutoResize": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.mouseBackForwardToNavigate": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.splitSizing": "distribute",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.pdf": "default"
    },
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always"
}
